@using EntityLayer.Concrete
@model List<Service>
@{

}

<h1>Hizmetler</h1>

<table class="table table-bordered">
	<tr>
		<th> #</th>
		<th> Başlık</th>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<td>@item.ServiceID</td>
			<td>@item.Title</td>
		</tr>
	}

</table>
<a href="/Test/AddService/" class="btn btn-outline-info">Yenü Hizmet girişi</a>

